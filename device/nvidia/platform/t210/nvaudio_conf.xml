          <ctl name="x Stereo ADC MIXR ADC1 Switch" val="0"/>
          <ctl name="x Stereo ADC MIXL ADC2 Switch" val="1"/>
          <ctl name="x Stereo ADC MIXR ADC2 Switch" val="1"/>
          <ctl name="x DMIC Switch" val="DMIC1"/>
          <ctl name="x ADC Boost Gain" val="2"/>
          <ctl name="x Int Mic Switch" val="1"/>
